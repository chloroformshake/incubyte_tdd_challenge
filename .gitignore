# Ignore Ruby on Rails backend files
/backend/log/*
/backend/tmp/*
/backend/db/*.sqlite3
/backend/db/*.sqlite3-journal
/backend/public/system/*
/backend/storage/*
/backend/vendor/bundle/
/backend/.env
/backend/config/master.key
/backend/config/credentials.yml.enc

# Ignore Ruby version files (managed by rbenv)
/.ruby-version
/.ruby-gemset

# Ignore Node.js/React frontend files
/frontend/node_modules/
/frontend/build/
/frontend/.env
/frontend/.env.local
/frontend/.env.development.local
/frontend/.env.test.local
/frontend/.env.production.local
/frontend/.DS_Store
/frontend/coverage/

# Ignore general files
*.log
*.swp
.DS_Store
*.sublime-project
*.sublime-workspace

# Ignore OS generated files
.DS_Store
.AppleDouble
.LSOverride

# Ignore IDE-specific files
.idea/
*.iml

# Ignore dependency lock files (optional, keep if you want to commit them)
# /backend/Gemfile.lock
# /frontend/package-lock.json